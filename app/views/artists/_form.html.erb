
<div class="container">
  <div class="col-md-12">
  <%= form_for(@artist, type: :file) do |f| %>
    <div class="form-group">
      <%= f.label :artist %>
      <%= f.text_field :artistn, class: 'form-control' %>
    </div>
    <div class = "input">
      <%= f.label :image %>
      <%= f.file_field :image %>
    </div>

    <div class="form-group">
      <%= f.submit "Save", class: 'btn btn-lg btn-success' %>
    </div>
  <% end %>
  </div>
</div>
